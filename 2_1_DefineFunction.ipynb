   "source": [
    "def execute_db(fname, sql_cmd):\n",
    "    conn = sqlite3.connect(fname)\n",
    "    c = conn.cursor()\n",
    "    c.execute(sql_cmd)\n",
    "    conn.commit()\n",
    "    conn.close()"
   ]
